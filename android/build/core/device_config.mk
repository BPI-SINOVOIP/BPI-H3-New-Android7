#add marco SW_CHIP_PLATFORM
#A80:  0x00
LOCAL_CFLAGS += -DA80_CHIP_PLATFORM=0x00
#H8:   0x02
LOCAL_CFLAGS += -DH8_CHIP_PLATFORM=0x02
#H3:   0x03
LOCAL_CFLAGS += -DH3_CHIP_PLATFORM=0x03
#H64: 0x04
LOCAL_CFLAGS += -DH64_CHIP_PLATFORM=0x04
#H5: 0x05
LOCAL_CFLAGS += -DH5_CHIP_PLATFORM=0x05
#H6: 0x06
LOCAL_CFLAGS += -DH6_CHIP_PLATFORM=0x06

ifeq (A80 , $(SW_CHIP_PLATFORM))
    LOCAL_CFLAGS += -DSW_CHIP_PLATFORM=0x00
endif
ifeq (H8 , $(SW_CHIP_PLATFORM))
    LOCAL_CFLAGS += -DSW_CHIP_PLATFORM=0x02
endif
ifeq (H3 , $(SW_CHIP_PLATFORM))
    LOCAL_CFLAGS += -DSW_CHIP_PLATFORM=0x03
endif
ifeq (H64 , $(SW_CHIP_PLATFORM))
    LOCAL_CFLAGS += -DSW_CHIP_PLATFORM=0x04
endif
ifeq (H5 , $(SW_CHIP_PLATFORM))
    LOCAL_CFLAGS += -DSW_CHIP_PLATFORM=0x05
endif
ifeq (H6 , $(SW_CHIP_PLATFORM))
    LOCAL_CFLAGS += -DSW_CHIP_PLATFORM=0x06
endif


#add marco BUSINESS_PLATFORM
#cmcc:  0x08
LOCAL_CFLAGS += -DPLATFORM_CMCCWASU=0x08
#tvd:   0x0A
LOCAL_CFLAGS += -DPLATFORM_TVD=0x0A

ifeq (cmccwasu , $(TARGET_BUSINESS_PLATFORM))
    LOCAL_CFLAGS += -DBUSINESS_PLATFORM=0x08
endif
ifeq (fvd , $(TARGET_BUSINESS_PLATFORM))
    LOCAL_CFLAGS += -DBUSINESS_PLATFORM=0x0A
