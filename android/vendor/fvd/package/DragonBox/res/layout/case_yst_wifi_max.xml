<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/case_yst_wifi_conn_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/case_yst_wifi_conn_name" />

    <TextView
        android:id="@+id/case_yst_wifi_strength"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/case_yst_wifi_conn_streagth" />

    <TextView
        android:id="@+id/case_yst_wifi_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/case_yst_wifi_conn_status" >
    </TextView>

    <TextView
        android:id="@+id/case_yst_wifi_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/case_yst_wifi_text" >
    </TextView>

</LinearLayout>